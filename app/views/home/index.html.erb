
<div class="center jumbotron">
  <h1>Welcome to Swink</h1>

  <h2>
    <%= form_tag '/sessions' do |f| %>
		<%= text_field_tag :email, nil, placeholder: "Enter Email", id: "login_email"%>
		<%= password_field_tag :password, nil, placeholder: "Enter Password", id: "password"%>
		<%= submit_tag "Login", :class => 'submit'%>
	<% end %>
  </h2>

  <%= link_to "Sign up now!", sign_up_path, class: "btn btn-lg btn-primary" %>

</div>




